

<table align="center" border="1" cellspacing="0">
  <tr>
    <th>Casenumber</th>
    <th>Status</th>
    <th>Type</th>
    <th>Case orgin</th>
    <th colspan="3">Permissions</th>
  </tr>

<% @cases.each do |case1| %>
  <tr>
    
	<td><%=h case1.casenumber %></td>
    <td><%=h case1.status %></td>
    <td><%=h case1.type1 %></td>
    <td><%=h case1.case_orgin %></td>
    <td><%= link_to 'Show', case1 %></td>
    <td><%= link_to 'Edit', edit_case_path(case1) %></td>
    <td><%= link_to 'Destroy', case1, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New case', new_case_path %>